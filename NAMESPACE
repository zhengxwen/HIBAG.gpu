# Load the shared object
useDynLib(HIBAG.gpu,
	gpu_init_proc,
	ocl_set_verbose,
	ocl_init_dev_list, ocl_dev_info, ocl_get_dev_param,
	ocl_select_dev, ocl_release_dev,
	ocl_set_kl_attempt, ocl_set_kl_clearmem, ocl_set_kl_build, ocl_set_kl_predict,
	ocl_build_init, ocl_build_done, ocl_set_local_size
)

# Export function names
exportPattern("^hla")
export(.onAttach)
export(.onLoad)

# Imports
import(methods)
import(HIBAG)
importFrom(stats, predict)
